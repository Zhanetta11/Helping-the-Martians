import java.util.Random;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        Random random = new Random();
        
        int distance = 7;
        int[] cargoLocations = new int[3];
        
        for (int i = 0; i < 3; i++) {
            cargoLocations[i] = random.nextInt(distance) + 1;
        }
        System.out.println ("The martians decided to put their cargo in 3 boxes of different sizes and bury them in different places on the way to the city. ");
    }
}
